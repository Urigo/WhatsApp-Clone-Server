{
  "name": "whatsapp-clone-server",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://Urigo@github.com/Urigo/WhatsApp-Clone-Server.git"
  },
  "scripts": {
    "generate": "gql-gen",
    "generate:watch": "nodemon --exec yarn generate -e graphql",
    "start:server": "ts-node index.ts",
    "start:server:watch": "nodemon --exec yarn start:server -e ts",
    "dev": "concurrently \"yarn generate:watch\" \"yarn start:server:watch\"",
    "start": "yarn generate && yarn start:server"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/graphql": "14.0.4",
    "@types/graphql-iso-date": "3.3.1",
    "@types/node": "10.12.18",
    "@types/pg": "^7.4.11",
    "concurrently": "4.1.0",
    "graphql-code-generator": "0.16.0",
    "graphql-codegen-typescript-common": "0.16.0",
    "graphql-codegen-typescript-resolvers": "^0.16.1",
    "nodemon": "1.18.9",
    "ts-node": "8.0.1",
    "typescript": "3.2.4"
  },
  "dependencies": {
    "@graphql-modules/core": "0.4.2",
    "@graphql-modules/di": "0.4.0",
    "@graphql-modules/sonar": "0.4.0",
    "apollo-server-express": "2.3.1",
    "body-parser": "1.18.3",
    "core-js": "3.0.0-beta.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "graphql": "14.0.2",
    "graphql-iso-date": "3.6.1",
    "moment": "^2.24.0",
    "pg": "7.8.0",
    "reflect-metadata": "0.1.12",
    "typeorm": "0.2.11"
  }
}
