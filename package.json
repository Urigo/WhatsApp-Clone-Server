{
  "name": "whatsapp-clone-server",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://Urigo@github.com/Urigo/WhatsApp-Clone-Server.git"
  },
  "scripts": {
    "generate": "gql-gen",
    "generate:watch": "nodemon --exec yarn generate -e graphql",
    "start:server": "ts-node index.ts",
    "start:server:watch": "nodemon --exec yarn start:server -e ts",
    "dev": "concurrently \"yarn generate:watch\" \"yarn start:server:watch\"",
    "start": "yarn generate && yarn start:server"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.2",
    "@types/graphql": "14.0.7",
    "@types/node": "11.9.5",
    "concurrently": "4.1.0",
    "graphql-code-generator": "0.17.0",
    "graphql-codegen-typescript-common": "0.17.0",
    "graphql-codegen-typescript-resolvers": "0.17.0",
    "nodemon": "1.18.10",
    "ts-node": "8.0.2",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "apollo-server-express": "2.4.6",
    "body-parser": "1.18.3",
    "core-js": "3.0.0-beta.16",
    "cors": "2.8.5",
    "express": "4.16.4",
    "graphql": "14.1.1",
    "moment": "2.24.0"
  }
}